#: ../../en/admin-guide/db-update-pum/index.rst:73
msgid "``pg_dump -Fc -f /path/to/dump/qgep_prod.dump -d \"service=qgep_prod\"``"
msgstr ""

#: ../../en/admin-guide/db-update-pum/index.rst:76
msgid "Download and install current database release"
msgstr ""

#: ../../en/admin-guide/db-update-pum/index.rst:78
